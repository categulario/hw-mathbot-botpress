{
  "version": "0.1",
  "catchAll": {
    "onReceive": [],
    "next": [
      {
        "condition": "event.nlu.intent.is('forget')",
        "node": "forget-my-name"
      }
    ]
  },
  "startNode": "start_bot",
  "nodes": [
    {
      "id": "bda0320009",
      "name": "welcome",
      "onEnter": [],
      "onReceive": [
        "initTable {\"name\":\"tableNumber\",\"value\":\"{{ event.texr }}\"}",
        "tagUser {\"tag\":\"tableNumber\",\"value\":\"{{event.text}}\"}",
        "setUserVariable {\"name\":\"tableNumber\",\"value\":\"{{event.text}}\",\"expiry\":\"never\"}",
        "getUserVariable {\"name\":\"tableNumber\",\"output\":\"$tableNumber\"}"
      ],
      "next": [
        {
          "condition": "isNaN(state.$tableNumber)",
          "node": "invalidInput"
        },
        {
          "condition": "state.$tableNumber > 0 && state.$tableNumber <=10",
          "node": "initTableQuestions"
        },
        {
          "condition": "state.$tableNumber <= 0 || state.$tableNumber > 10",
          "node": "invalidNumber"
        }
      ]
    },
    {
      "id": "7099199f90",
      "name": "invalidInput",
      "next": [
        {
          "condition": "always",
          "node": "welcome"
        }
      ],
      "onEnter": [
        "say #!builtin_text-B8I3ds",
        "say #!builtin_text-49ixXn"
      ],
      "onReceive": null
    },
    {
      "id": "c69ee2d71f",
      "name": "tableQuestion",
      "next": [
        {
          "condition": "!state.toChange && state.isCorrect",
          "node": "goodAnswer"
        },
        {
          "condition": "!state.toChange && !state.isCorrect",
          "node": "badAnswer"
        },
        {
          "condition": "state.toChange",
          "node": "initTableQuestions"
        }
      ],
      "onEnter": [
        "tableQuestion {}",
        "say #!builtin_text-yOrr5X"
      ],
      "onReceive": [
        "checkAnswer {\"resp\":\"{{event.text}}\"}"
      ]
    },
    {
      "id": "4f50a2e9fd",
      "name": "forget-my-name",
      "next": [],
      "onEnter": [
        "say #!builtin_text-TtzrCV",
        "resetUserVariable {\"name\":\"nickname\"}",
        "say #!builtin_text-kKQ8C3"
      ],
      "onReceive": null
    },
    {
      "id": "a54a82eb7c",
      "name": "start_bot",
      "onEnter": [
        "getUserVariable {\"name\":\"nickname\",\"output\":\"$r\"}",
        "say #!builtin_text-XZxS8o",
        "say #!builtin_text-AcmttM"
      ],
      "onReceive": null,
      "next": [
        {
          "condition": "state.$r !== null",
          "node": "welcome"
        },
        {
          "condition": "true",
          "node": "ask-name"
        }
      ]
    },
    {
      "id": "d29fc6b771",
      "name": "ask-name",
      "next": [
        {
          "condition": "true",
          "node": "welcome"
        }
      ],
      "onEnter": [
        "say #!builtin_text-z0J9qh"
      ],
      "onReceive": [
        "setUserVariable {\"name\":\"nickname\",\"value\":\"{{event.text}}\",\"expiry\":\"never\"}",
        "getUserVariable {\"name\":\"nickname\",\"output\":\"$r\"}"
      ]
    },
    {
      "id": "b77c47de7b",
      "name": "invalidNumber",
      "next": [
        {
          "condition": "always",
          "node": "welcome"
        }
      ],
      "onEnter": [
        "",
        "say #!builtin_text-AMBWx5"
      ],
      "onReceive": null
    },
    {
      "id": "693c6bbe90",
      "name": "goodAnswer",
      "next": [
        {
          "condition": "always",
          "node": "tableQuestion"
        }
      ],
      "onEnter": [
        "say #!builtin_text-cbJsVV",
        "nextQuestion {}"
      ],
      "onReceive": null
    },
    {
      "id": "72424e210a",
      "name": "badAnswer",
      "next": [
        {
          "condition": "always",
          "node": "tableQuestion"
        }
      ],
      "onEnter": [
        "say #!builtin_text-lQlXD~"
      ],
      "onReceive": null
    },
    {
      "id": "815de7bc24",
      "name": "initTableQuestions",
      "next": [
        {
          "condition": "always",
          "node": "tableQuestion"
        }
      ],
      "onEnter": [
        "say #!builtin_text-4ICRHC"
      ],
      "onReceive": null
    }
  ]
}